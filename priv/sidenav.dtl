<div id="sidenav" class="nav">
  <form>
    <input id="search" type="text" autocomplete="off" />
  </form>
  <span id="hide-nav-button" onclick="closeNav()">&#9776;</span>
  <div id="sidenav-title">
    <h1><a href="index.html">{{ application }}</a></h1>
    <h2>{{ version }}</h2>
  </div>
  {% if modules|length > 0 %}
  <ul id="sidenav-modules">
    {% for m in modules %}
    <li><a href="{{ m.name }}.html">{{ m.name }}</a></li>
    {% endfor %}
  </ul>
  {% endif %}
</div>
